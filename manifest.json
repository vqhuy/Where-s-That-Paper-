{
  "name": "Where's That Paper?",
  "version": "1.0.2",
  "manifest_version": 2,
  "description": "Automatically adds viewed academic papers to your favorites.",
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/jquery/jquery.js",
	  "src/bg/bibtexParser.js",
	  "src/bg/scrapers.js",
	  "src/bg/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    }
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
	"bookmarks",
	"tabs"
  ]
}